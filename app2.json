{
    "devtools_port": 9090,
    "startup_app": {
        "name": "BigW2",
        "uuid": "BigW2",
        "url": "http://localhost:8081/index.html",
        "urlof": "https://openfin.co",
        "taskbarIconGroup": "bigw",
        "showTaskbarIcon": true,
        "autoShow": true,
        "defaultHeight": 500,
        "defaultWidth": 500,
        "maxHeightx": 600,
        "frame": true,
        "resizable": true,
        "plugins": true,
        "waitForPageLoad": true,
        "saveWindowState": true,
        "frameConnectxx": "all",
        "transparent": false,
        "contextMenuxx": true,
        "contextMenuSettings": {
          "enable": true,
          "devtools": true,
          "reload": true
        },
        "shadow": false,
        "customWindowAlert": true,
        "backgroundThrottling": true,
        "backgroundColor": "red",
        "spellCheck": true,
        "experimental": {
          "api": {
            "iframe": {
              "crossOriginInjection": false
            }
          },
          "v2Apix": true,
          "node": false
        },
        "resizeRegionx": {
          "sides": {"bottom": false, "left": false}
        },
        "customData": {
          "name": "OpenFin",
          "address": "25 Broadway"
        },
        "permissions": {
          "System": {
            "launchExternalProcess": true
          }
        },    
        "accelerator": { "zoom": true, "devtools": true }
    },
    "appAssets": [
      {
        "src": "https://cdn.openfin.co/demos/java/java-asset.zip",
        "alias": "java-asset",
        "target": "run.bat",
        "version": "1.0.0"
      }
    ],  
    "runtime": {
        "arguments": "--security-realm=test --disable-gpu-compositing --enable-aggressive-domstorage-flushing --force-device-scale-factor=1 --inspect --v=1 ",
        "version": "8.56.30.71",
        "version9": "9.61.38.43",
        "version": "stable"
    }
